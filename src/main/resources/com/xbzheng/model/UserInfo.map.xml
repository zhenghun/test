<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xbzheng.model.UserInfo">
    <resultMap id="userInfo" type="com.xbzheng.model.UserInfo">
        <id column="id" property="id"/>
        <result property="username" column="username"/>
        <result property="password" column="password"/>
    </resultMap>
    <select id="findAllUserInfo" resultMap="userInfo">
        <![CDATA[
          select * from t_user
        ]]>
    </select>

    <insert id="insertUserInfo" parameterType="com.xbzheng.model.UserInfo">
        <![CDATA[
          INSERT INTO T_USER(ID, USERNAME, PASSWORD) VALUES(#{id},#{username},#{password})
        ]]>
    </insert>
</mapper>